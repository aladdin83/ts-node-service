{
  "name": "ts-node-service",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "copyfiles -u 1 \"./src/**/*.html\" \"./src/**/*.css\" \"./dist\"",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "accepts": "^1.3.5",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "connect-mongodb-session": "^2.0.7",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "escape-html": "^1.0.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "fast-json-patch": "^2.0.7",
    "g": "^2.0.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.4.6",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "s": "^0.1.1"
  },
  "devDependencies": {
    "@types/accepts": "^1.3.5",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/connect-mongodb-session": "0.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/escape-html": "0.0.20",
    "@types/express": "github:types/npm-express",
    "@types/express-session": "^1.15.11",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.3.9",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/shelljs": "^0.8.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "hygen": "^2.1.1",
    "nodemon": "^1.18.9",
    "shelljs": "^0.8.3",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "resolutions": {}
}
